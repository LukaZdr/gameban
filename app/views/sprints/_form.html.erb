<%= simple_form_for sprint, url: project_sprints_path do |f| %>
  <%= f.input :project_id, html: { input: project_id }, as: :hidden %>
  <%= f.input :start_time, as: :datetime %>
  <%= f.input :end_time, as: :datetime %>
  <%= f.input :quality_1 %>
  <%= f.input :quality_2 %>
  <%= f.input :quality_3 %>
  <%= f.input :quality_4 %>
  <%= f.input :quality_5 %>
  <%= f.submit 'Save' %>
<% end %>
